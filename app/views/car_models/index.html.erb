<div class="card mt-3">
  <div class="card-body">
    <h5 class="card-title">Car Models</h5>

    <!-- Default Table -->
    <table class="table">
      <thead>
      <tr>
        <th scope="col">S.No</th>
        <th scope="col">Name</th>
      </tr>
      </thead>
      <tbody>
      <% @car_models.each_with_index do |car_model, index| %>
        <tr>
          <th scope="row"><%= index + 1 %></th>
          <td><%= car_model.name %></td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>
